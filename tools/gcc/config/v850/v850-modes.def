CC_MODE (CC_FPU_LT);
CC_MODE (CC_FPU_LE);
CC_MODE (CC_FPU_GT);
CC_MODE (CC_FPU_GE);
CC_MODE (CC_FPU_EQ);
CC_MODE (CC_FPU_NE);

/* Floating point comparison modes.
   Individual modes for each comparison type are needed because the
   V850 encodes the type into the CMPF instruction, rather than into
   the conditional brnch that will follow, but the COMPARE rtl does
   not take a comparison type parameter, only a mode.  */

CC_MODE (CC_FPU);

CC_MODE (CC_FPU_UN);
CC_MODE (CC_FPU_UNLT);
CC_MODE (CC_FPU_UNLE);
CC_MODE (CC_FPU_UNEQ);

/* Integer comparison modes.  */

CC_MODE (CC_Z);
CC_MODE (CC_ZS);
CC_MODE (CC_ZSO);
